{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "web#build": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "PAYLOAD_SECRET",
        "PAYLOAD_DATABASE_URI",
        "NEXT_PUBLIC_CLIENT_URL",
        "NEXT_PUBLIC_POSTHOG_KEY",
        "NEXT_PUBLIC_POSTHOG_HOST",
        "NEXT_PUBLIC_GOOGLE_MAPS",
        "BLOB_READ_WRITE_TOKEN",
        "POSTGRES_URI"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    }
  }
}
